<?xml version="1.0" encoding="UTF-8"?>
<style2tagmap
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="urn:public:dita4publishers.org:namespaces:word2dita:style2tagmap ../../doctypes/style2tagmap/xsd/style2tagmap.xsd"
  xmlns="urn:public:dita4publishers.org:namespaces:word2dita:style2tagmap">
   <title>Style-to-Tag Map</title>
   <documentation
      xmlns:html="http://www.w3.org/1999/xhtml">
      <html:div
         xmlns="http://www.w3.org/1999/xhtml">        
         <p>NOTE: The first non-skipped paragraph in the document should styled as "Title", which is mapped to &lt;title&gt;
            within &lt;pubmap&gt; and triggers the generation of the root output map.</p>
      </html:div>
   </documentation>
   <styles>

      <!-- define outputs first -->
      <output
         doctype-public="urn:pubid:astd.com/doctypes/dita/pubmap"
         doctype-system="pubmap.dtd"
         mapType="pubmap"
         name="pubmap"
         prologType="topicmeta"/>

      <output
         doctype-public="urn:pubid:astd.com/doctypes/dita/topic"
         doctype-system="topic.dtd"
         name="topic"/>
      
      <output
         doctype-public="urn:pubid:astd.com/doctypes/dita/chapter"
         doctype-system="chapter.dtd"
         name="chapter"/>
     
      <output
         doctype-public="urn:pubid:astd.com/doctypes/dita/subsection"
         doctype-system="subsection.dtd"
         name="subsection"/>

      <paragraphStyle
         level="0"
         structureType="mapTitle"
         containerType="pubtitle"
         styleName="ATD DOCTYPE BOOK"
         tagName="mainpubtitle">
         <mapProperties format="pubmap" prologType="topicmeta" tagName="title"/>
         <!--         <topicrefProperties topicrefType="topicref"/>
         <topicProperties
            bodyType="body"
            format="topic"
            topicDoc="yes"
            topicType="topic"
            outputclass="Professional_Book"/>
-->      </paragraphStyle>

      <paragraphStyle
         level="1"
         structureType="topicTitle"
         styleName="ATD CHAPTER"
         tagName="title"
         outputclass="Title">
         <topicrefProperties topicrefType="topicref"/>
         <topicProperties
            bodyType="body"
            format="chapter"
            topicDoc="yes"
            topicType="chapter"/>
      </paragraphStyle>
      
      <paragraphStyle
         level="1"
         structureType="topicTitle"
         styleName="ATD TOC"
         tagName="title"
         outputclass="Title">
         <topicrefProperties topicrefType="topicref"/>
         <topicProperties
            bodyType="body"
            format="topic"
            topicDoc="yes"
            topicType="topic"
            outputclass="TOC"/>
      </paragraphStyle>

      <paragraphStyle
         level="1"
         structureType="topicTitle"
         styleName="ATD INDEX"
         tagName="title"
         outputclass="Title">
         <topicrefProperties topicrefType="topicref"/>
         <topicProperties
            bodyType="body"
            format="topic"
            topicDoc="yes"
            topicType="topic"
            outputclass="INDEX"/>
      </paragraphStyle>


      <paragraphStyle
         level="2"
         structureType="topicTitle"
         styleName="Overview"
         tagName="title">
         <topicrefProperties topicrefType="topicref"/>
         <topicProperties
            bodyType="body"
            format="subsection"
            topicDoc="no"
            topicType="subsection"
            outputclass="Section"/>
      </paragraphStyle>

      <paragraphStyle 
         styleName="ATD 1st Paragraph Example/Tool" 
         tagName="p" 
         outputclass="1st_Paragraph_Example/Tool"
         topicZone="body" 
         level="1"/>
      
      <paragraphStyle 
         styleName="ATD 1s Paragraph" 
         tagName="p" 
         outputclass="1st_Paragraph"
         topicZone="body" 
         level="1"/>

      <paragraphStyle 
         styleName="ATD body" 
         tagName="p" 
         outputclass="Body"
         topicZone="body" 
         level="1"/>

      
      <paragraphStyle 
         styleName="ATD body Example/Tool" 
         tagName="p" 
         outputclass="Body_Example/Tool"
         topicZone="body" 
         level="1"/>      

      <paragraphStyle 
         styleName="ATD bullet Example/Tool" 
         tagName="li" 
         containerType="ul"
         containerOutputclass="Bullet_Example/Tool"
         topicZone="body" 
         level="1"/>
      
      <paragraphStyle 
         styleName="ATD bullet level 2 Example/Tool" 
         tagName="li" 
         containerType="ul"
         containerOutputclass="Bullet_Example/Tool_L2"
         topicZone="body" 
         level="2"/>      

      <paragraphStyle 
