<rsuite-plugin
        id="astd-main-toolkit-plugins"
        dependsOn="rsuite-dita-ot-plugin,rsuite-dita-support-plugin,atd-main-plugin">
        
    <extensionProvider id="rsuite.WebService">
        <staticWebService root="/astd-toolkit-plugins" path="/WebContent" />
    </extensionProvider>
    
    <extensionProvider id="rsuite.ditaOpenToolkit">
        <ditaOpenToolkit toolkitName="default">
            <toolkitPlugin>
                <zipfile>astd-main-toolkit-plugins-plugin.zip</zipfile>
            </toolkitPlugin>
        </ditaOpenToolkit>
    </extensionProvider>
    
</rsuite-plugin>